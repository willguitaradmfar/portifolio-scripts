  "version": "0.2.0",
  "configurations": [      
    {
      "name": "Execute script",
      "type": "node",
      "request": "launch",
      "program": "./node_modules/orki-core-script",
      "args": ["execute", "-f", "${file}"],
      "console": "internalConsole"
    }, {
      "name": "Publish script",
      "type": "node",
      "request": "launch",
      "program": "./node_modules/orki-core-script",
      "args": ["publish", "-f", "${file}"],
      "console": "internalConsole"
    }]
}